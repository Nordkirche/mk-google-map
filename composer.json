{
    "name": "nordkirche/nk-google-map",
    "type": "typo3-cms-extension",
    "description": "TYPO3 Extension f√ºr die Darstellung von Karten mit Markern aus der NAPI",
    "license": "GPL-2.0",
    "author": [
        {
            "name": "Holger McCloy",
            "role": "Developer",
            "company": "netzleuchten GmbH"
        }
    ],
    "require": {
        "typo3/cms-core": "^10.4 || ^11.5 || ^12.4"
    },
    "autoload": {
        "psr-4": {
            "Nordkirche\\NkGoogleMap\\": "Classes"
        }
    },
    "config": {
        "bin-dir": ".build/bin",
        "discard-changes": true,
        "optimize-autoloader": true,
        "vendor-dir": ".build/vendor"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "nk_google_map"
        }
    },
    "require-dev": {
        "typo3/coding-standards": "^0.7.1"
    }
}
